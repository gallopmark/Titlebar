<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="tb_titleText" format="string" />
        <attr name="tb_titleTextSize" format="dimension" />
        <attr name="tb_titleTextStyle" format="enum">
            <enum name="normal" value="1" />
            <enum name="italic" value="2" />
            <enum name="bold" value="3" />
        </attr>
        <attr name="tb_titleTextColor" format="color" />
        <attr name="tb_titleAppearance" format="reference" />
    </declare-styleable>
</resources>